            	<div class="dropdown-divider"></div>
          	</div>
          </div>
          <div class= 'nav-item dropdown'>
          	<a class="nav-link dropdown-toggle" href="#" id="navbarDropdown03" role="button"	data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Hjælp</a>
          	<div class="dropdown-menu" aria-labelledby="navbarDropdown03">
            	<a class="dropdown-item" href="#">Action</a>
            	<a class="dropdown-item" href="#">Baggrunds materiale</a>
            	<div class="dropdown-divider"></div>
            	<a class="dropdown-item" data-toggle='modal' data-target="#aboutModal" href="#">Om..</a>
          	</div>
          </div>
      	</div>
    	</div>

    	{% if our_form is defined %}


      {{ form_start(our_form, {'attr': {'class': 'form-inline  my-2 my-lg-0'} }) }}
{#
    		<div class="custom-control custom-switch px-3" >
      		{{ form_widget(our_form.latin, {'type': 'checkbox', 'attr': {'class': 'custom-control-input', type: 'checkbox', empty_data: 'false'  }}) }}
      		<label class="custom-control-label" for="form_latin">Latin</label>
    		</div>
#}
    		{{ form_widget(our_form.year, {'attr': {'class': 'form-control mr-sm-2', 'placeholder': 'Årstal', 'type': 'number', 'aria-label': 'Search'} }) }}
        {{ form_widget(our_form.send, {'label': 'Vælg', 'attr': {'class': 'btn btn-outline-primary my-2 my-sm-0'} })}}
   			{{ form_rest(our_form) }}
      {{ form_end(our_form )}}
      {% endif %}
